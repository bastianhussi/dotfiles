{
  "workbench.colorTheme": "onehalf-light",
  "keyboard.dispatch": "keyCode",
  "window.zoomLevel": 0,
  "editor.fontSize": 20,
  "editor.fontFamily": "'Fira Code Retina', 'JetBrains Mono', 'Cascadia Code', 'Droid Sans Mono', 'monospace'",
  "editor.fontLigatures": true,
  "editor.minimap.enabled": false,
  "editor.renderWhitespace": "all",
  "editor.lineNumbers": "relative",
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modifications",
  "vim.leader": "<space>",
  "vim.useSystemClipboard": true,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useCtrlKeys": true,
  "vim.surround": true,
  "vim.sneak": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<leader>", "f"],
      "after": [],
      "commands": [{ "command": "editor.action.formatDocument" }]
    },
    {
      "before": ["<leader>", "d", "b"],
      "after": [],
      "commands": [{ "command": "editor.debug.action.toggleBreakpoint" }]
    },
    {
      "before": ["<leader>", "d", "d"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.start" }]
    },
    {
      "before": ["<leader>", "d", "c"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.continue" }]
    },
    {
      "before": ["<leader>", "d", "o"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.stepOver" }]
    },
    {
      "before": ["<leader>", "d", "i"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.stepInto" }]
    },
    {
      "before": ["<leader>", "d", "O"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.stepOut" }]
    },
    {
      "before": ["<leader>", "d", "p"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.pause" }]
    },
    {
      "before": ["<leader>", "d", "r"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.restart" }]
    },
    {
      "before": ["<leader>", "d", "q"],
      "after": [],
      "commands": [{ "command": "workbench.action.debug.stop" }]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "debug.console.fontSize": 18,
  "debug.toolBarLocation": "docked",
  "markdown.preview.fontSize": 18,
  "terminal.integrated.fontSize": 18,
  "tabnine.experimentalAutoImports": true,
  "python.defaultInterpreterPath": "/usr/bin/python3",
  "python.linting.enabled": true,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/usr/bin/flake8-3",
  "python.linting.banditEnabled": true,
  "python.linting.banditPath": "/usr/bin/bandit-3",
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Path": "/usr/bin/autopep8-3",
  "python.sortImports.path": "/usr/bin/isort-3",
  "go.useLanguageServer": true,
  "remote.SSH.connectTimeout": 45,
  "remote.SSH.useLocalServer": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
