{
  "suggest.minTriggerInputLength": 1,
  "codeLens.enable": true,
  "suggest.languageSourcePriority": 100,
  "snippets.priority": 99,
  "coc.preferences.formatOnSaveFiletypes": ["*"],
  "coc.preferences.rootPatterns": [
    ".git",
    "Cargo.toml",
    "package.json",
    "setup.py",
    "go.mod",
    "pom.xml",
    "build.gradle"
  ],
  "coc.preferences.bracketEnterImprove": true,
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["typescript"]
  },
  "diagnostic.enableMessage": "always",
  "diagnostic.errorSign": "ðŸš¨",
  "diagnostic.warningSign": "ðŸ”¥",
  "diagnostic.infoSign": "ðŸ”Ž",
  "diagnostic.hintSign": "ðŸ’¡",
  "python.jediEnabled": false,
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Path": "/usr/bin/flake8-3",
  "python.linting.banditEnabled": true,
  "python.linting.banditPath": "/usr/bin/bandit",
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Path": "/usr/bin/autopep8-3",
  "python.sortImports.path": "/usr/bin/isort-3",
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "eslint.autoFixOnSave": true,
  "eslint.format.enable": true,
  "prettier.eslintIntegration": true
}
